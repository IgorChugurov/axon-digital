    "keyrevoke": "tsx tools/keyrevoke.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "p-queue": "^8.0.1",
    "playwright": "^1.47.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.0",
    "redis": "^4.7.0",
    "axios": "^1.7.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2"
  }
